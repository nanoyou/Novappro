<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.akagawatsurunaki.novappro.mapper.UserMapper">
    <select id="selectById" resultType="com.github.akagawatsurunaki.novappro.model.database.User">
        select * from user where user.id = #{id}
    </select>
</mapper>